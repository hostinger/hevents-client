{
    "name": "hostinger/hevents-client",
    "type": "library",
    "description": "Hevents Client - Easily connect to Hevents in your PHP project",
    "keywords": ["hostinger","hevents","web service","client"],
    "license": "MIT",
    "authors": [
        {
            "name": "Hostinger",
            "homepage": "https://hostinger.com"
        }
    ],
    "require": {
        "php": ">=5.6",
        "guzzlehttp/guzzle": "6.*",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "5.*"
    },
    "autoload": {
        "psr-4": {
            "Hostinger\\Hevents\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hostinger\\Hevents\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
